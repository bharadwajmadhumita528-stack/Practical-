#include <iostream>
using namespace std;

class Queue {
    int arr[5];     // Array to hold elements
    int front, rear; // Front and rear indexes
    int size;        // Maximum size of the queue

public:
    Queue() {
        size = 5;
        front = -1;
        rear = -1;
    }

    // Check if queue is empty
    bool isEmpty() {
        return front == -1;
    }

    // Check if queue is full
    bool isFull() {
        return rear == size - 1;
    }

    // Add element to the queue
    void enqueue(int value) {
        if (isFull()) {
            cout << "Queue is full. Cannot add " << value << "." << endl;
            return;
        }

        if (isEmpty()) {
            front = 0;  // first element is added
        }

        rear++;
        arr[rear] = value;
        cout << "Enqueued " << value << " into queue." << endl;
    }

    // Remove element from the queue
    void dequeue() {
        if (isEmpty()) {
            cout << "Queue is empty. Cannot remove element." << endl;
            return;
        }

        cout << "Dequeued " << arr[front] << " from queue." << endl;

        // If only one element was left
        if (front == rear) {
            front = rear = -1;
        } else {
            front++;
        }
    }

    // Display all elements in the queue
    void display() {
        if (isEmpty()) {
            cout << "Queue is empty." << endl;
            return;
        }

        cout << "Queue elements: ";
        for (int i = front; i <= rear; i++) {
            cout << arr[i] << " ";
        }
        cout << endl;
    }
};

int main() {
    Queue q;

    q.enqueue(10);
    q.enqueue(20);
    q.enqueue(30);
    q.enqueue(40);
    q.enqueue(50);
    q.enqueue(60); // will show queue is full

    q.display();

    q.dequeue();
    q.dequeue();
    q.display();

    return 0;
}
