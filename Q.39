#include <iostream>
using namespace std;

class Stack {
    int arr[5];      // Array to store stack elements
    int top;         // Index of the top element
    int size;        // Maximum size of the stack

public:
    Stack() {
        size = 5;
        top = -1;    // Stack is empty initially
    }

    // Function to check if the stack is full
    bool isFull() {
        return top == size - 1;
    }

    // Function to check if the stack is empty
    bool isEmpty() {
        return top == -1;
    }

    // Push operation: add an element to the top of the stack
    void push(int value) {
        if (isFull()) {
            cout << "Stack is full. Cannot push " << value << "." << endl;
        } else {
            top++;
            arr[top] = value;
            cout << "Pushed " << value << " into stack." << endl;
        }
    }

    // Pop operation: remove the top element from the stack
    void pop() {
        if (isEmpty()) {
            cout << "Stack is empty. Cannot pop." << endl;
        } else {
            cout << "Popped " << arr[top] << " from stack." << endl;
            top--;
        }
    }

    // Display the stack elements
    void display() {
        if (isEmpty()) {
            cout << "Stack is empty." << endl;
            return;
        }

        cout << "Stack elements (top to bottom): ";
        for (int i = top; i >= 0; i--) {
            cout << arr[i] << " ";
        }
        cout << endl;
    }
};

int main() {
    Stack s;

    s.push(10);
    s.push(20);
    s.push(30);
    s.push(40);
    s.push(50);
    s.push(60);  // This will show 'Stack is full'

    s.display();

    s.pop();
    s.pop();
    s.display();

    return 0;
}
