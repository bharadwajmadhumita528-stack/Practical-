#include <iostream>
using namespace std;

int main() {
    int size;
    cout << "Enter the size of the array: ";
    cin >> size;

    int arr[size];

    // Asking user to initialize the array
    cout << "Enter " << size << " numbers:" << endl;
    for (int i = 0; i < size; i++) {
        cin >> arr[i];
    }

    int n;
    cout << "Enter the position n to get the nth element (starting from 1): ";
    cin >> n;

    // Checking valid position
    if (n >= 1 && n <= size) {
        cout << "The " << n << "th element is: " << arr[n - 1] << endl;
    } else {
        cout << "Invalid position!" << endl;
    }

    return 0;
}
